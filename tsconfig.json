{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"outDir": "lib",
		"noEmit": true,
		"resolveJsonModule": true,
		"importHelpers": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"skipLibCheck": true
	}
}
